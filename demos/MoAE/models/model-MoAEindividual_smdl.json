{
  "Name": "auditory",
  "BIDSModelVersion": "1.0.0",
  "Description": "GLM and contrasts the FIL MoAE dataset in subject space",
  "Input": {
    "task": [
      "auditory"
    ],
    "space": [
      "individual"
    ]
  },
  "Nodes": [
    {
      "Level": "Run",
      "Name": "run_level",
      "GroupBy": [
        "run",
        "subject"
      ],
      "Model": {
        "X": [
          "trial_type.listening",
          "trans_?",
          "rot_?"
        ],
        "HRF": {
          "Variables": [
            "trial_type.listening"
          ],
          "Model": "DoubleGamma"
        },
        "Type": "glm",
        "Options": {
          "HighPassFilterCutoffHz": 0.0078
        },
        "Software": [
          {
            "SPM": {
              "SerialCorrelation": "AR(1)",
              "HRFderivatives": "none"
            }
          }
        ]
      },
      "DummyContrasts": {
        "Test": "t",
        "Contrasts": [
          "trial_type.listening"
        ]
      },
      "Contrasts": [
        {
          "Name": "listening_inf_baseline",
          "ConditionList": [
            "trial_type.listening"
          ],
          "Weights": [
            -1
          ],
          "Test": "t"
        }
      ]
    }
  ]
}
